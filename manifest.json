{
  "name": "HannahTech",
  "version": "1.0",
  "description": "description for Hannah-Tech",
  "background": {
    "scripts": [ "background.js" ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [ "*://*/*" ],
      "js": ["contentScript.js" ],
      "all_frames": true
    }
  ],
  "browser_action": {
    "default_icon": "HannahTech.png"
  },
  "permissions": [
    "contextMenus"
  ],
  "manifest_version": 2
}
